{
   "name": "youtube-supabase",
   "$schema": "../../../node_modules/nx/schemas/project-schema.json",
   "sourceRoot": "libs/youtube/supabase/src",
   "projectType": "library",
   "targets": {
      "gen:types": {
         "executor": "nx:run-commands",
         "options": {
            "command": "supabase gen types typescript --project-id boydmgzwehvxxvydovbv --schema public --schema storage --schema auth > ./src/types.ts",
            "cwd": "libs/youtube/supabase"
         }
      },
      "build": {
         "executor": "@nx/js:swc",
         "outputs": ["{options.outputPath}"],
         "options": {
            "outputPath": "dist/libs/youtube/supabase",
            "main": "libs/youtube/supabase/src/index.ts",
            "tsConfig": "libs/youtube/supabase/tsconfig.lib.json",
            "assets": ["libs/youtube/supabase/*.md"]
         }
      },
      "lint": {
         "executor": "@nx/linter:eslint",
         "outputs": ["{options.outputFile}"],
         "options": {
            "lintFilePatterns": ["libs/youtube/supabase/**/*.ts"]
         }
      }
   },
   "tags": []
}
